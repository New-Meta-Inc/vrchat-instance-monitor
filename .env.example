# VRChatの認証情報
# ユーザー名またはメールアドレスを入力してください
VRCHAT_USERNAME=your_username_or_email

# VRChatのパスワードを入力してください
VRCHAT_PASSWORD=your_password

# 監視するワールドID
# デフォルト: wrld_7bb60bf6-3c69-4039-a5d6-0cbbda092290
VRCHAT_WORLD_ID=wrld_7bb60bf6-3c69-4039-a5d6-0cbbda092290

# 出力ファイル名（オプション）
OUTPUT_FILE=vrchat_instances.txt

# データ収集間隔（分単位）
INTERVAL_MINUTES=10

# デバッグモード（trueで詳細ログを出力）
DEBUG=false
